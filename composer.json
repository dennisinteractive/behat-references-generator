{
  "name": "dennisdigital/entity_reference_generator",
  "type": "behat-extension",
  "description": "Generates non-existing referenced entities when creating content",
  "keywords": ["behat", "content", "entities"],
  "homepage": "https://github.com/dennisinteractive/behat-drupal-entity-reference-generator",
  "license": "MIT",
  "require": {
    "drupal/drupal-extension": "~3.1"
  },
  "require": {
    "geraintluff/jsv4": "1.0.1",
    "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
    "drupal/drupal-extension": "dev-master#7756b54f",
    "drupal/drupal-driver": "dev-master#74d4ef1377"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.0"
  },
  "autoload": {
    "psr-0": {
      "DennisDigital\\EntityReferenceGenerator": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "master-dev"
    }
  }
}
